class Solution {
    public int uniquePaths(int m, int n) {
        long result = 1;
        int smaller = Math.min(m - 1, n - 1);
        
        for (int i = 0; i < smaller; i++) {
            result *= (m + n - 2 - i);
            result /= (i + 1);
        }
        
        return (int) result;
    }
}
