class Solution {
    public int searchInsert(int[] nums, int target) {
        int s=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]>=target){
                    s=i;
                    break;
                }
                else if(nums[i]!=target && nums[i]==nums[nums.length-1]){
                    s=nums.length;
                    break;
                }
        }
        return s;
    }
}
